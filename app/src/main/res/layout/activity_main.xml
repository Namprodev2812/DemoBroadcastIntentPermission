<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#3F51B5"
    android:gravity="center"
    android:orientation="vertical">
    <Button
        android:id="@+id/button_sendintentex"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/send_intent_Ex" />
    <Button
        android:id="@+id/button_sendintentview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/send_intent_im_view" />
    <Button
        android:id="@+id/button_sendintentmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/send_intent_im_mail" />
</LinearLayout>
